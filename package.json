{
  "name": "Fulcrum",
  "version": "0.0.80",
  "electron": "1.6.6",
  "description": "Fulcrum",
  "homepage": "http://github.com/fulcrumapp/fulcrum-desktop",
  "main": "app/index.js",
  "scripts": {
    "test": "mocha ./test/setup.js ./test/test.js",
    "prepublish": "npm run clean && npm run build",
    "lint": "eslint src/**/*.js",
    "assets": "ncp src/assets app/assets && ncp src/index.html app/index.html",
    "watch:assets": "cpx 'src/**/*.{html,png,jpg,css}' app --watch --verbose",
    "build": "babel src --out-dir app --source-maps=inline",
    "watch:js": "babel src --out-dir app --source-maps=inline --watch",
    "watch": "concurrently 'yarn watch:js' 'yarn watch:assets'",
    "clean": "rm -rf dist",
    "pack": "build --dir",
    "dist": "build"
  },
  "author": "Zac McCormick <zac.mccormick@gmail.com> (http://github.com/zhm)",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/fulcrumapp/fulcrum-desktop.git"
  },
  "eslintConfig": {
    "extends": "fulcrum"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/register": "^7.14.5",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "cpx": "^1.5.0",
    "eslint": "^3.17.1",
    "eslint-config-fulcrum": "^1.5.4",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^2.4.5",
    "ncp": "^2.0.0",
    "should": "^8.3.1",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "async": "^2.4.0",
    "bluebird": "^3.3.5",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "csv-string": "^2.3.0",
    "delimit-stream": "^0.1.0",
    "ejs": "^2.4.1",
    "exif": "^0.6.0",
    "filesize": "^3.3.0",
    "fulcrum-core": "^0.10.13",
    "fulcrum-query-sql": "^0.0.96",
    "fulcrum-schema": "^2.0.11",
    "glob": "^7.1.1",
    "inquirer": "^1.0.2",
    "lodash": "^4.11.1",
    "minidb": "^0.15.1",
    "mixmatch": "0.0.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.23.0",
    "mv": "^2.1.1",
    "node-uuid": "^1.4.7",
    "pg": "^6.1.5",
    "pg-format": "^1.0.4",
    "progress": "^1.1.8",
    "request": "^2.72.0",
    "rimraf": "^2.5.2",
    "sanitize-filename": "^1.5.3",
    "source-map-support": "^0.4.14",
    "tempy": "^0.1.0",
    "through2": "^2.0.3",
    "uuid": "^2.0.2",
    "yargs": "^7.1.0"
  },
  "keywords": [
    "fulcrum"
  ]
}
